{
  "extends": "@tsconfig/react-native/tsconfig.json",    
  "compilerOptions": {
    "skipLibCheck": true,   
    "target": "ES6",
    "allowSyntheticDefaultImports": true,
    "jsx": "react-native",
    "lib": ["dom", "esnext"],
    "moduleResolution": "node",
    "noEmit": true,
    "resolveJsonModule": true,
    "suppressImplicitAnyIndexErrors": true,
    "strict": true,
    "strictFunctionTypes": false,                         
    "baseUrl": "./",
    "paths": {
      "~assets/*": [
        "./src/assets/*"
      ],
      "~navigation/*": [
        "./src/navigation/*"
      ],
      "~components/*": [
        "./src/components/*"
      ],
      "~screens/*": [
        "./src/screens/*"
      ],
      "~utils/*": [
        "./src/utils/*"
      ],
      "~startup/*": [
        "src/utils/startup/*"
      ],
      "~root/*": [
        "./src/*"
      ]
    }
  }
}
